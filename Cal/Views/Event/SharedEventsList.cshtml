@model List<SharedEvents>

@if (!Model.Any())
{
    <h1>Вы ничем не делились.</h1>

}
else
{
    <h1>Ссылки на события, которыми вы делились:</h1>
        <ul>
            @foreach (var SharedEvent in Model)
            {
                <li style="display: flex; align-items: center;">
                    <a style="color:black; text-decoration:none;font-size:25px" href="https://localhost:7039/ShareEvents/@SharedEvent.UniqueIdentifier">
                        https://localhost:7039/ShareEvents/@SharedEvent.UniqueIdentifier
                    </a>
                </li>
            }
        </ul>
}